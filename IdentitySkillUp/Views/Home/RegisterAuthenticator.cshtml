@model IdentitySkillUp.Models.RegisterAuthenticatorModel

@{
    ViewData["Title"] = "RegisterAuthenticator";
}

<h1>RegisterAuthenticator</h1>

<h4>RegisterAuthenticatorModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <p>Your authenticator key is: @Model.AuthenticatorKey</p>
        <form asp-action="RegisterAuthenticator">
            <div asp-validation-summary="All" class="text-danger"></div>
            <input asp-for="AuthenticatorKey" type="hidden" />
            <div class="form-group">
                <label asp-for="Code" class="control-label"></label>
                <input asp-for="Code" class="form-control" />
                <span asp-validation-for="Code" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

